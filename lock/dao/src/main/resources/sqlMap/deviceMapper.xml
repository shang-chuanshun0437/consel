<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.weiyi.lock.dao.mapper.DeviceMapper" >
    <resultMap id="BaseResultMap" type="com.weiyi.lock.dao.entity.Device" >
        <id column="device_num" property="deviceNum" jdbcType="BIGINT" />
        <result column="device_name" property="deviceName" jdbcType="VARCHAR" />
    </resultMap>

    <insert id="insert" parameterType="com.weiyi.lock.dao.entity.Device">
       INSERT INTO
       		device
       		(device_num,device_name)
       	VALUES
       		(#{deviceNum}, #{deviceName})
    </insert>

</mapper>